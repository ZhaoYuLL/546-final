<h1>Wardrobe</h1>
<nav>
	<ul>
		<li><a href="./wardrobe">Wardrobe</a></li>
		<li><a href="./closet">Closet</a></li>
		<li><a href="./favorites">Favorites</a></li>
	</ul>
</nav>
<div class="wardrobes">
	{{#each wardrobes}}
		<div class="wardrobe">
			<h2>{{name}}</h2>
			<div class="fitposts">
				{{#each fitposts}}
					<div class="fitpost">
						<img src="{{imageUrl}}" alt="{{description}}" />
						<p>{{description}}</p>
					</div>
				{{/each}}
			</div>
		</div>
	{{/each}}
</div>